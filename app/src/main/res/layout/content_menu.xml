<?xml version="1.0" encoding="utf-8"?>
        <!--Layouts Externo (Vertical): primer layout-->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation ="vertical"
    tools:context="com.example.personal.comunitarias.Menu"
    android:background="@drawable/fondo"
    tools:showIn="@layout/app_bar_menu">



    <TableLayout

        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1" >


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="10dp"
            android:paddingEnd="20dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp">

            <LinearLayout

                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingRight="5dp">


                <Button
                    android:id="@+id/idNoticias"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/noticias2" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="NOTICIAS"

                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="5dp">

                <Button
                    android:id="@+id/idRedesSociales"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/preguntasfrecuentes" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="ENCUENTRANOS EN"

                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold" />
            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingBottom="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="20dp">

            <LinearLayout

                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingRight="5dp">


                <Button
                    android:id="@+id/idPP"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/participacion_ciudadana" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="PARTICIPACION CIUDADANA"

                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="5dp">

                <Button
                    android:id="@+id/idControlSocial"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/control_social" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"

                    android:text="CONTROL SOCIAL"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold" />
            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop = "20dp"
            android:paddingBottom = "10dp"
            >

            <LinearLayout

                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingRight="5dp"
                >

                <Button
                    android:id="@+id/idTransparencia"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/lucha_anti_corrupcion" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="TRANSPARENCIA"
                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout

                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingLeft="5dp">


                <Button
                    android:id="@+id/idCorrupcion"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/lucha_anti_corrupcion" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="LUCHA CONTRA LA CORRUPCION"

                    android:textColor="#000000"
                    android:textSize="15sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </TableRow>

        <ImageView
            android:id="@+id/iddd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/footer" />
    </TableLayout>
</LinearLayout>